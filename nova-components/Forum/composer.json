{
    "name": "acme/forum",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0",
        "laravel/framework": "^10.0",
        "kalnoy/nestedset": "^6.0",
        "doctrine/dbal": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Acme\\Forum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acme\\Forum\\ToolServiceProvider",
                "Acme\\Forum\\ForumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
