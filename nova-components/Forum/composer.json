{
    "name": "acme/forum",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0",
        "riari/laravel-forum": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Acme\\Forum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acme\\Forum\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
