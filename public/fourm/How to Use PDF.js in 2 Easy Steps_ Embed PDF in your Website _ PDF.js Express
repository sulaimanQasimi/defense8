</span><span>  removeElement(&amp;#</span><span style="color:#880000">39</span><span>;secondaryToolbarToggle&amp;#</span><span style="color:#880000">39</span><span>;)
</span><span>  removeElement(&amp;#</span><span style="color:#880000">39</span><span>;scaleSelectContainer&amp;#</span><span style="color:#880000">39</span><span>;)
