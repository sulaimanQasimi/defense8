{
    "name": "sq/fingerprint",
    "description": "Fingerprint capture, storage, and matching package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SQ Team",
            "email": "info@sq.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sq\\Fingerprint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sq\\Fingerprint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sq\\Fingerprint\\FingerprintServiceProvider"
            ],
            "aliases": {
                "Fingerprint": "Sq\\Fingerprint\\Facades\\Fingerprint"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
